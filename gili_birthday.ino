const unsigned char* aliens_bitmaps [] = {
a01_default,
a02_cannonbolt,
a04_upchuck,
a05_benmummy,
a06_benvicktor,
a07_ghostfreak,
a08_ditto,
a09_waybig,
a10_eyeguy
};

const int alienCount = (sizeof(aliens_bitmaps) / sizeof(aliens_bitmaps[0]));
const int analogInPin = A0;  // ESP8266 Analog Pin ADC0 = A0

int alien_bitmap_index = -1;

int lastSelector = 0;

void loop() 
{
  int sensorValue = analogRead(analogInPin);
  int selector = map(sensorValue, 0, 1023, 0, (alienCount - 1) * 2);

  Serial.print("selector=");
  Serial.print(selector);
  Serial.println();

  if (selector % 2 == 1) return;

  if (selector  == lastSelector) return;

  lastSelector = selector;
//  alien_bitmap_index = (alien_bitmap_index + 1) % (sizeof(aliens_bitmaps) / sizeof(unsigned char*));
  alien_bitmap_index = (selector / 2) % (sizeof(aliens_bitmaps) / sizeof(aliens_bitmaps[0]));
  
  // Display logo
  display.clearDisplay();
  display.drawBitmap(0, 0,  aliens_bitmaps[alien_bitmap_index], 128, 64, WHITE);
  display.display();
//  delay(5000);
//  display.clearDisplay();
}
